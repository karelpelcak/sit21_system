{
    "name": "@patrick115/create-app",
    "version": "1.0.1",
    "description": "Create a SvelteKit/TypeScript/Discord Bot app",
    "bin": "lib/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn ./src/index.ts",
        "build": "mkdir -p build && tsc -p tsconfig.json ",
        "start": "node ./build/index.js",
        "clear": "rm -r build",
        "depl": "pnpm run build && node ./deploy.js && cp build/* ~/scripts && cp CreatePatrick115App ~/scripts && cd ~/scripts && pnpm i && chmod +x CreatePatrick115App",
        "publ": "mkdir -p lib && tsc -p tsconfig.json --outDir lib && npm publish --access public"
    },
    "keywords": [],
    "author": "Patrik MintÄ›l",
    "license": "ISC",
    "dependencies": {
        "cli-color": "^2.0.3",
        "enquirer": "^2.4.1",
        "node-fetch": "^3.3.2"
    },
    "devDependencies": {
        "@types/cli-color": "^2.0.3",
        "@types/inquirer": "^9.0.3",
        "@types/node": "^20.8.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.2.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/patrick11514/CreateApp.git"
    },
    "bugs": {
        "url": "https://github.com/patrick11514/CreateApp/issues"
    },
    "homepage": "https://github.com/patrick11514/CreateApp#readme"
}
